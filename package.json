{
  "name": "adga",
  "version": "3.6.0-beta.7",
  "description": "Unofficial ADGA (https://app.adga.org/) node.js library (SDK)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bloomkd46/ADGA.git"
  },
  "bugs": {
    "url": "https://github.com/bloomkd46/ADGA/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0 --fix",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "yarn build",
    "watch": "nodemon --watch src --ext .ts,.json --exec 'npm run build'",
    "test": "tsc test.ts && node  --enable-source-maps test.js",
    "pull-check": "ts-node ./scripts/check.ts",
    "release": "ts-node ./scripts/release.ts",
    "fix-version": "ts-node ./scripts/version.ts"
  },
  "keywords": [
    "ADGA",
    "goats",
    "dairy"
  ],
  "dependencies": {
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "simple-git": "^3.28.0",
    "ts-node": "^10.3.0",
    "typedoc": "^0.22.18",
    "typescript": "^4.4.4"
  }
}